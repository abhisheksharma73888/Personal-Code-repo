	<script type="text/javascript">

    $(document).ready(function() {

      $('.j-switch-language').on('click',function(){
        $(this).toggleClass('active');
        $('.j-language-item').toggle();
      });

      $('.j-language-item').on('click','a',function(e){
            if($(this).data('actived')){
                $('.j-language-item').toggle();
                e.preventDefault();
            }else{
                $('.j-switch-language').attr('href',$(this).data('locale'))
                $('.j-switch-language').toggleClass('active');
            }
        });

      $('.j-drop-menu-trigger').on('click',function(){
        $('.j-drop-menu').slideToggle();
        $(this).toggleClass('on');
      });

      $('.j-drop-menu').on('click',function(){
        $('.j-drop-menu').slideUp();
        $('.j-drop-menu-trigger').removeClass('on');
      });

      $(document).on('click',function(e){
        if( $(e.target).parents('.mobile-menu').length == 0 ){
          $('.j-drop-menu').slideUp();
          $('.j-drop-menu-trigger').removeClass('on');
        }
      })
    });
  </script>
