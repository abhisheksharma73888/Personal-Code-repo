  <body class="qor-theme-slideout" style="background-color: rgba(0, 0, 0, 0.5);border:none;">
    
    <script>
      QOR_Translations = window.QOR_Translations || {};
      QOR_Translations.okButton = 'OK'
      QOR_Translations.cancelButton = 'Cancel'
      QOR_Translations.serverError = 'Server error, please try again later!'
      QOR_Translations.slideoutCloseWarning = 'You have unsaved changes on this slideout. If you close this slideout, you will lose all unsaved changes. Are you sure you want to close the slideout?'
    </script>
    <script src="./../../../../10.10.30.5_7000/admin/assets/javascripts/qor_admin_default.js"></script>
    <script src="./../../../../10.10.30.5_7000/admin/assets/javascripts/qor_demo.js"></script>
    

    <script type="text/javascript">
      $(document).ready(function(){
        function closeWidgetEditBox() {
          setTimeout(function() {
            window.parent.document.getElementById('qor-actionbar-iframe').classList.remove("show");
            window.parent.document.querySelector('body').classList.remove("open-actionbar-editor");
          }, 350);
        }

        $(document).on('bottomsheetSubmitComplete.qor.bottomsheets', function(){
          closeWidgetEditBox();
        })
        
        $('.qor-slideout').on('slideoutSubmitComplete.qor.slideout' , function() {
          setTimeout(function() {
            parent.window.location.reload();
          }, 300);
        }).on('slideoutClosed.qor.slideout', function(){
          closeWidgetEditBox();
        });
      });
    </script>
  </body>
